  "scripts": {
    "prepare": "ts-patch install -s",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --fix && prettier --write ./src",
    "lsm": "open https://statecharts.io/inspect?server=localhost:8888",
    "wsm": "powershell.exe /c 'start https://statecharts.io/inspect?server=localhost:8888'",
    "sm": "SMD=1 node --experimental-specifier-resolution=node --loader ts-node/esm src/menuStateMachine.ts",
    "play": "node --experimental-specifier-resolution=node --loader ts-node/esm src/cli/bestPlayCLI.ts",
    "compare": "node --experimental-specifier-resolution=node --loader ts-node/esm src/cli/buildComparisonCLI.ts",
    "best": "node --experimental-specifier-resolution=node --loader ts-node/esm src/cli/bestBuildCLI.ts",
    "smokePlay": "node --experimental-specifier-resolution=node --loader ts-node/esm src/cli/bestPlayCLI.ts --json='src/cli/sampleGame.json' --iterations=5 --population=10 --seed=paco",
    "smokeCompare": "node --experimental-specifier-resolution=node --loader ts-node/esm src/cli/buildComparisonCLI.ts --builds=src/cli/samplebuilds.json --encounterCount=5 --iterations=5 --population=10",
    "smokeBest": "node --experimental-specifier-resolution=node --loader ts-node/esm src/cli/bestBuildCLI.ts --encounters=2 --players=5 --iter=5 --iterPerGame=5 --pop=10 --popPerGame=10"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "prettier": "2.6.2",
    "ts-patch": "^2.0.1",
    "tsconfig-paths": "^3.11.0"
  }
}
